{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "",
    "VStockCache": "localhost:6379"
  },
  "Token": {
    "SecurityKey": "",
    "Issuer": "",
    "Audience": ""
  },
  "FinHubCryptoSocket": {
    "Uri": "wss://ws.finnhub.io?token=",
    "ApiKey": ""
  },
  "BinanceCryptoSocket": {
    "TrackedSymbols": [

    ],
    "StreamUri": "wss://stream.binance.com:9443/stream?streams="
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext","WithMachineName","WithThreadId"],
    "WriteTo": [
      {"Name": "Console"},
      {
        "Name": "Seq",
        "Args": { "serverUrl": "" }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "DefaultConnection",
          "sinkOptions": {
            "tableName": "Logs",
            "autoCreateSqlTable":true,
            "schemaName": "dbo"
          }
        }
      }
    ]
  }
} 
